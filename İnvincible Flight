local UserInputService = game:GetService("UserInputService")

local isMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled

if isMobile then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Meteaga1aga/Universal/refs/heads/main/MobileFly"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Meteaga1aga/Universal/refs/heads/main/FlyR15"))()
end
